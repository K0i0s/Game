//Izquierda
void left(float x, float y, float w, float h) {
  noStroke();
  strokeWeight(1.5);
  fill(255, 10, 255, 125);
  rect(x, y, w, h);
  fill(0);
  stroke(0);
  line(x, y+(h/4), x+(w/4), y+h);
  line(x+(w/4), y+(h/4), x+w, y+(h/4));
  line(x, y+(h/4), x+(w/4), y);
  line(x+(w/4), y, x+(w/4), y+h);
  noStroke();
}

//Arriba
void up(float x, float y, float w, float h) {
  noStroke();
  strokeWeight(1.5);
  fill(255, 10, 255, 125);
  rect(x, y, w, h);
  fill(0);
  stroke(0);
  line(x, y+(h/4), x+w, y+(h/4));
  line(x+(w/4), y, x, y+(h/4));
  line(x+(w/4), y, x+w, y+(h/4));
  line(x+(w/4), y+(w/4), x+(w/4), y+h);
  noStroke();
}
//Abajo
void down(float x, float y, float w, float h) {
  noStroke();
  strokeWeight(1.5);
  fill(255, 10, 255, 125);
  rect(x, y, w, h);
  fill(0);
  stroke(0);
  line(x, y+(h/4), x+w, y+(h/4));
  line(x+(w/4), y+h, x, y+(h/4));
  line(x+(w/4), y+h, x+w, y+(h/4));
  line(x+(w/4), y+(w/4), x+(w/4), y);
  noStroke();
}
