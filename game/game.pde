void right(float x, float y, float w, float h) {
  noStroke();
  strokeWeight(1.5);
  fill(255, 10, 255, 125);
  rect(x, y, w, h);
  fill(0);
  stroke(0);
  line(x+w, y+(h/4), x+(w/4), y+h);
  line(x+w, y+(h/4), x+(w/4), y);
  line(x+(w/4), y, x+(w/4), y+h);
  line(x+(w/4), y+(w/4), x, y+(h/4));
  noStroke();
}


